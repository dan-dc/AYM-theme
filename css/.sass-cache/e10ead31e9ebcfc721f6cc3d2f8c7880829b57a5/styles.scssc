/*************************************************************************

	AWARDS FOR YOUNG MUSICIANS
	
	Developed by Dan Moe & Ruyman Rodriguez 
	for Design Culture - http://www.design-culture.co.uk/ (c) 2013
	
	MODULE: 	styles.scss
	PURPOSE:	main site styles
	
*************************************************************************/

/* CODE MAP
-------------------------------------------------------------------------

	^1	IMPORTS 
	
	^2	SASS VARIABLES 
	
	^3	GLOBAL CLASSES 
	
	^4	STRUCTURE
	
	    ^4a CORE
	        ^4a1 masthead
	        ^4a2 navigation
	        ^4a3 footer
	        ^4a4 sitemap
	        
	    ^4b HOME

  ^5  TEXT


/*	^1 IMPORTS
*************************************************************************/

	@import url("reset.css");
	@import url(http://fonts.googleapis.com/css?family=Open+Sans:400,400italic,600,700,600italic,700italic);
	

/* ^2 SASS VARIABLES
*************************************************************************/
	
	// Colours
	$Black : #000;
	$DarkBlack : lighten($Black, 13%);
  $MediumBlack : lighten($Black, 20%);
  $LightBlack : lighten($Black, 80%);
  $UltraLightBlack : lighten($Black, 95%);
  $White : #FFF;
  $Orange : #FF6600;
  $Purple : #6C276A;
  
  // Globals
  $GlobalWidth : 1020px;
  $StickyHeight : 200px;
  $GlobalPadding : 20px;
  $GlobalLinkSize : 0.7em;
  
  // Fonts
  @mixin serifFont {
    font-family : "Century", Georgia, "Times New Roman", Times, serif;
  }

  @mixin sansSerifFont {
    font-family : "Open Sans", Helvetica, Arial, sans-serif;
  }

  @mixin formFields {
    height : 15px;
    padding : 5px ;
    border : 0;
    @include sansSerifFont;
    font-size : $GlobalLinkSize;
  }
  
  @mixin arrowLinks {
    height : 25px;
    width : 25px;
    float : left;
    border : 0;
    background : $Orange url(../img/sprite.png) no-repeat -83px -74px;
      &:hover {
        background-color : $LightBlack;
      }  
  }
  
  @mixin resetSpacing {
    padding : 0;
    margin : 0;
  }
  
  @mixin bringToFront {
    position : relative;
    z-index : 10;
  }

/* ^3	GLOBAL CLASSES
*************************************************************************/

	::-moz-selection {
		background : #b3d4fc;
		text-shadow : none;
	}
	
	::selection {
		background : #b3d4fc;
		text-shadow : none;
	}
	
	.clearfix:before,
	.clearfix:after {
		content : " ";
		display : table;
	}
	
	.clearfix:after {
		clear : both;
	}
	
	.oldbrowser {
		padding : 0.4em 0;
    margin : 0;
		text-align : center;
    background : $Orange;
    color : $White;
	}
	
	.oldbrowser a {
	  color : $Black;
	  &:hover {
	    color : $LightBlack;
	  }
	}
	
	hr {
		display : block;
		height : 1px;
		border : 0;
		border-top : 1px solid $MediumBlack;
		margin : 1em 0;
		padding : 0;
	}
	
	a {
	  text-decoration : none;
	}
	
	.hide {
	  display : none;
	}
	
  button[type=submit] {
    @include arrowLinks;
  }


/* ^4	STRUCTURE
*************************************************************************/

// ^4a core
  html, body {
    height : 100%;
    background : $UltraLightBlack;
    @include sansSerifFont;
  }
  
  #wrapper {
    height : 100%;
    min-height: 100%;
    height: auto !important;
    margin: 0 auto (-$StickyHeight);
    width : $GlobalWidth; 
    background : $White;
  }
  
  #content {
    min-height : 400px; // might need removing to resolve sidebar height conflicts
  }
  
  #push {
    height : $StickyHeight;
  }
  
  // ^4a1 masthead
  #masthead {
    height : 125px;
    background : $White;
    @include bringToFront;
    
    h1 {
        height : 80px;
        width : 190px;
        padding : 10px 20px;
        margin : 0;
        float : left;
        
        a {
            display : block;
            height : 80px;
            width : 190px;
            background : url(../img/sprite.png) no-repeat 0px 10px;
        }        
     }
  }

  #header {
    height : 100px;
    width : 790px;
    float : right;
  }
  
  #smallLinks {
    height : 30px;
    padding-right : 10px;
    
    ul {
      list-style : none;
      @include resetSpacing;
    }
    
    > ul {
      float : right;

      > li {
        display : inline;
        height : 10px;
        padding : 10px;
        font-size : $GlobalLinkSize;
        font-weight: 600;
        border-left : 1px dotted $MediumBlack;
        color : $MediumBlack;
        
        a {
          color : $MediumBlack;
          &:hover {
            color : $Orange;
          }
        }
      }
    }    
  }

  #socialLinks {
    display : inline-block;
    position : relative;
    top : 6px;

    li {
      height : 20px;
      width : 20px;
      float : left;
      margin-left : 5px;
      
      a {
        display : block;
        height : 20px;
        width : 20px;
      }
    }
  }
  
  .linkedin {
    background : url(../img/sprite.png) no-repeat -10px -77px;
    &:hover {
      background-position :  -10px -97px;
    }
  }

  .twitter {
    background : url(../img/sprite.png) no-repeat -33px -78px;
    &:hover {
      background-position :  -33px -98px;
    }
  }

  .facebook {
    background : url(../img/sprite.png) no-repeat -52px -77px;
    &:hover {
      background-position :  -52px -97px;
    }
  }
  
  #search-form {
    width : 220px;
    float : right;
    padding-top : $GlobalPadding;

    input[type=text] {
      @include formFields;
      width : 165px;
      float : left;
      background : $UltraLightBlack;
    }
  }
  
  // ^4a2 navigation
  #nav {
    height : 25px;
    background : $DarkBlack;
    clear : both;
    
    input[type=text] {
      width : 200px;
      float : left;
      background : $MediumBlack;
      color : $White;
      @include formFields;
      padding-left : 20px;
    }
    
    ul {
      float : right;
      @include resetSpacing;

      li {
          font-size : $GlobalLinkSize;
          float : left;
          @include resetSpacing;
          
          a {
            display : inline-block;
            margin : 0;
            padding : 5px 1em;
            height : 15px;
            color : $White;
            &:hover {
              color : $Orange;
            }
          }
       }
    } 
  }

  .angels a {
    background : $Orange;
    &:hover {
        color : $DarkBlack !important;
    }
  }
  
  // ^4a3 footer
  #footer {
    height : $StickyHeight;
    width : $GlobalWidth; 
    margin : auto;
    background : $Orange;
    @include bringToFront;
        
    nav {
      height : 175px;
    }
    
    section {
      height : 20px;
      padding : 5px 0 0 1.75em;
      background : $DarkBlack;
      color : $White; 
      
      a {
        color : $White; 
        &:hover {
          color : $Orange;
        }      } 
    } 
    
    h6 {
      float : left;
    } 
    
    p {
      font-size : $GlobalLinkSize - 0.1;
      margin : 0 0 10px 0 ;
      color : $White;
      
      a {
        color : $White;
        &:hover {
          color : $Black;
        }
      }
    }
  }
  
  // ^4a4 sitemap
  #sitemap {
    @include resetSpacing;
    padding : 15px 20px 0;
    float : left;
    
    li a {
        padding : 0 1.5em;
        color : $White;
        &:hover {
          color : $Black;
        }
     } 
     
     ul + ul {
       padding-top : 23px !important;
     }   
  }
  
  #sitemap > li {
    display : inline-block;
    float : left;
    height : 160px;
    margin : 0;
    font-size : $GlobalLinkSize - 0.1;
    border-right : 1px dotted $White;
    &:last-child {
      border-right : none;
    }
  }

  #sitemap > li > a {
    font-weight: 600;
    color : $White;
  }
  
  #sitemap > li ul {
    @include resetSpacing;
  }
  
  #sitemap > li > ul > li  {
    clear : both;
    padding : 0;
  }  
  
  .right-column {
    width : 180px;
    float : right; 
    padding : 0 0.7em;
  }

  #footer nav .right-column {
    border-left : 1px dotted $White;
    margin-top : 15px ;
    height : 160px;
  }

  /* PAGE SPECIFIC */
  
  // ^4b HOME
  .home #wrapper {
    background : $MediumBlack;
  }
  
  .home #content {
    padding : $GlobalPadding;
  }
  
/* ^5  TEXT
*************************************************************************/
  h1, h2, h3 , h4, h5, h6 {
    font-weight : normal;
  }
  
  h6 {
    @include resetSpacing;
    font-size : $GlobalLinkSize - 0.1;
  }
