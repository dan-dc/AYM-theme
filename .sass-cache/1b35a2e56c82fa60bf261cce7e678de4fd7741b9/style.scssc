  h6 {
    @include sansSerifFont; 
    color: $White;
    font-size : $GlobalLinkSize - 0.1;
  }
  
  #content li, p {
    font-size: 0.8em;
  }

  p a {
      color: $Orange;
      
      &:hover {
        text-decoration : underline;
      }
  }
  
  #content li a:hover {
      color: $Orange;
  }
  
  .sidebar ul {
    margin: 0 0 $GlobalPadding 0;
    padding: 0 0 $GlobalPadding 0;
    font-size: $GlobalLinkSize + 0.2;
    list-style : none;
  }
  
  #ItemsGallery h3,
  #ItemsGallery h4 {
    @include sansSerifFont; 
  }
