      > li:first-child ul {
        padding-left : 0;
      }
  
      ul + ul {
         padding-top : 23px !important;
      }   
    }
    
    .menu > li {
      display : inline-block;
      float : left;
      height : 160px;
      margin : 0;
      font-size : $GlobalLinkSize - 0.1;
      border-right : 1px dotted $White;
  
      &:last-child {
        border-right : none;
      }
    }
  
    .menu > li > a {
      font-weight: 600;
      color : $White;
    }
    
    .menu > li ul {
      @include resetSpacing;
    }
    
    .menu > li > ul > li  {
      clear : both;
      padding : 0;
    }  

    .menu-item-226,
