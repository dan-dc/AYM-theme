  /* ^4e1 Isotope filtering */
  
  .isotope-item {
    z-index: 2;
  }
  
  .isotope-hidden.isotope-item {
    pointer-events: none;
    z-index: 1;
  }
  
  /* Isotope CSS3 transitions */
  
  .isotope,
  .isotope .isotope-item {
    -webkit-transition-duration: 0.8s;
       -moz-transition-duration: 0.8s;
         -o-transition-duration: 0.8s;
            transition-duration: 0.8s;
  }
  
  .isotope {
    -webkit-transition-property: height, width;
       -moz-transition-property: height, width;
         -o-transition-property: height, width;
            transition-property: height, width;
  }
  
  .isotope .isotope-item {
    -webkit-transition-property: -webkit-transform, opacity;
       -moz-transition-property:    -moz-transform, opacity;
         -o-transition-property:         top, left, opacity;
            transition-property:         transform, opacity;
  }
  
  /* disabling Isotope CSS3 transitions */
  
  .isotope.no-transition,
  .isotope.no-transition .isotope-item,
  .isotope .isotope-item.no-transition {
    -webkit-transition-duration: 0s;
       -moz-transition-duration: 0s;
         -o-transition-duration: 0s;
            transition-duration: 0s;
  }

  /* ^4f CONTACT */
 #MainContent {
   
   form {
     padding-top : $GlobalPadding;
     margin-top : $GlobalPadding;
     border-top : $DarkDottedBorder;
     
     fieldset {
         border : none;
         padding : 0;
         margin : 0 0 20px 0;
         border-bottom : $DarkDottedBorder;
         
         legend {
             display : none;
         }
     }

     label {
       width : 22%;
       height : 25px;

       float : left;
       clear : left;
       font-size : $GlobalLinkSize;
       padding : 5px 0;
       margin-bottom : 20px;
       
 /*      &:last-of-type {
         margin-top : 18px
       } */

     }
     
     input[type=text],
     textarea {
       width : 73%;
       padding : 2%;
       float : right;
       //border : 1px solid $LightBlack;
       border : none;
       background : $UltraLightBlack;
       margin-bottom : 20px;
       font-size : $GlobalLinkSize;
    
     }
     
     select {
         float : right;
         margin-bottom: 20px;
//         background : $UltraLightBlack;
//         border : none;
         width : 77%;
         font-size : 0.8em;
         margin-top : 3px;
         height : auto;
         padding : 5px 0;
     }
     
     .twoCol { 
         float : left;
         width : 100%;
            
         input[type=text],
         textarea, select {
             clear : none;
             float : left;
             width : 20%; 
             
             &.second {
                 width : 24%;
                 float : right;
             }  
         }
         label {
             clear : none;
             float : left;
             width : 23%;
             
             &.second {
                padding-left : $GlobalPadding; 
                width : 17%; 
             } 
         }
     }
     
     textarea {
         margin-bottom: 0;
     }
     
     input[type=text] {
        height : 15px;
     }

      footer {
        width : 77%;
        float : right;
        
        p.caption {
            font-size : $GlobalLinkSize;
        }
      }
      
      input[type=submit] {
        border : none;
        height : 23px;
        width : 110px;
        padding-left : 6%;
        background : transparent;
        text-align : left;
        font-size : $GlobalLinkSize;
        position : relative;
        z-index : 1;
        @include sansSerifFont;
        color : $White;
        &:hover {
          //color : $Orange;
        }
      }
   }
   
    .send-arrow {
      position : relative;
      display : inline-block;
      background : $Orange url(img/sprite.png) no-repeat 0px -74px;
        &:hover {
          background-color : $LightBlack;
          .buttonBG {
              //background : $DarkBlack;
          }
        } 
        .buttonBG {
            height : 25px;
            width : 80px;
            position : absolute;
            top : 0;
            z-index : 0;
            background : $MediumBlackLight;
        }
      }   
    
 }
 
 .contact #LeftSidebar p {
   color : $White;
   margin : 0;
 }
 
 /* ^4g DONATE */
.donation #MainContent {
  width : 730px;
 }

#ways-to-donate {
  border-top : $DarkDottedBorder;
  margin-top : $GlobalPadding;
  
  .image {
    width : 170px;
    float : left;
  }
  
  .text {
    width : 540px;
    float : right;
  }
  
  .donate-wrap {
    border-bottom : $DarkDottedBorder;
    padding : 10px 0;
    position : relative;
  }
  
  .readMore {
    @include readMore;
  }
}
      
/* ^5  TEXT
*************************************************************************/
  h1, h2, h3 , h4, h5, h6 {
    @include resetSpacing;    
    @include serifFont; 
    color: $Orange;
    font-weight : normal;
    
    a {
      color: $Orange;
    }
  }
  
  h1 {
      font-size: 1.5em;      
  }

  h2 {
      font-size: 1.1em;      
  }
  
  h3 {
      font-size: 1em;
      margin-bottom : 0.5em;
  }
  
  h4 {
      font-size: 0.9em;
  }
    
  h5 {
    @include sansSerifFont; 
    font-size : 0.75em;
  }
  
  h6 {
    @include sansSerifFont; 
    color: $White;
    font-size : $GlobalLinkSize - 0.1;
  }
  
  em {
    font-weight : 600;
  }
  
  #content li, p {
    font-size: 0.8em;
  }

  p a {
      color: $Orange;
      
      &:hover {
        text-decoration : underline;
      }
  }
  
  #content li a:hover {
      color: $Orange;
  }
  
  .sidebar {
    p {
      color : $White;
      font-size: 0.7em;
    }
    
    ul {
      margin: 0 0 $GlobalPadding 0;
      padding: 0 0 $GlobalPadding 0;
      font-size: $GlobalLinkSize + 0.2;
      list-style : none;
    }
  }
  
  #ItemsGallery h3,
  #ItemsGallery h4 {
    @include sansSerifFont; 
  }
