/* ^4	STRUCTURE
*************************************************************************/

// ^4a core
  html, body {
    height : 100%;
    background : $UltraLightBlack;
    @include sansSerifFont;
  }
  
  #wrapper {
    height : 100%;
    min-height: 100%;
    height: auto !important;
    margin: 0 auto (-$StickyHeight);
    width : $GlobalWidth; 
    background : $White;
    position : relative;
  }
  
  #content {
    min-height : 400px; // might need removing to resolve sidebar height conflicts
    position : relative;
    z-index : 1;
  }
  
  #push {
    height : $StickyHeight;
  }
  
  // ^4a1 masthead
  #masthead {
    height : 125px;
    background : $White;
    @include bringToFront;
    
    h1 {
        height : 80px;
        width : 190px;
        padding : 10px 20px;
        margin : 0;
        float : left;
        
        a {
            display : block;
            height : 80px;
            width : 190px;
            background : url(img/sprite.png) no-repeat 0px 10px;
        }        
     }
  }

  #header {
    height : 100px;
    width : 790px;
    float : right;
  }
  
  #smallLinks {
    height : 30px;
    padding-right : 10px;
    
    ul {
      list-style : none;
      @include resetSpacing;
    }
    
    > ul {
      float : right;

      > li {
        display : inline;
        height : 10px;
        padding : 10px;
        font-size : $GlobalLinkSize;
        font-weight: 600;
        border-left : 1px dotted $MediumBlack;
        color : $MediumBlack;
        
        a {
          color : $MediumBlack;
          &:hover {
            color : $Orange;
          }
        }
      }
    }    
  }

  #socialLinks {
    display : inline-block;
    position : relative;
    top : 6px;

    li {
      height : 20px;
      width : 20px;
      float : left;
      margin-left : 5px;
      
      a {
        display : block;
        height : 20px;
        width : 20px;
      }
    }
  }
  
  .linkedin {
    background : url(img/sprite.png) no-repeat -10px -77px;
    &:hover {
      background-position :  -10px -97px;
    }
  }

  .twitter {
    background : url(img/sprite.png) no-repeat -33px -78px;
    &:hover {
      background-position :  -33px -98px;
    }
  }

  .facebook {
    background : url(img/sprite.png) no-repeat -52px -77px;
    &:hover {
      background-position :  -52px -97px;
    }
  }
  
  #search-form {
    width : 220px;
    float : right;
    padding-top : $GlobalPadding;

    input[type=text] {
      @include formFields;
      width : 165px;
      float : left;
      background : $UltraLightBlack;
    }
  }
  
  // ^4a2 navigation
  #nav {
    height : 25px;
    background : $DarkBlack;
    clear : both;
    
    input[type=text] {
      width : 200px;
      float : left;
      background : $MediumBlackLight;
      color : $White;
      @include formFields;
      padding-left : 20px;
    }
    
    > div > ul { // top level
      position : relative;
      float : right;
      @include resetSpacing;

      > li {
          font-size : $GlobalLinkSize;
          float : left;
          @include resetSpacing;
          //position : relative;

          > a {
            display : inline-block;
            margin : 0;
            padding : 5px 0.8em;
            height : 15px;
          }
          
          > ul { // secondary level
            position : absolute;
            background : $DarkBlack;
            background : rgba(0,0,0,0.7);
            @include resetSpacing;
            padding : 10px 0;
            line-height : 1.8em;
            display : none;
            
            > li {
              position : relative;
              
              a {
                padding : 5px 1em;
              }
              
              > ul { // tertiary level
                @include resetSpacing;
                background : $MediumBlack;
                background : rgba(0,0,0,0.6);
                position : absolute;
                top : 0;
                left : 100%;
                width : auto;
                
                > li {
                  white-space:nowrap;
                }
              }
            }
          } 
       }
     } 

    .drop-down:after {
      color : $Orange;
      content : "\203A";
      display : inline-block;
      position : relative;
      right : 5px;
      top : 0;
      transform:rotate(90deg);
      -ms-transform:rotate(90deg); /* IE 9 */
      -webkit-transform:rotate(90deg); /* Safari and Chrome */      
    }
    
    .drop-out:after {
      color : $Orange;
      content : "\203A";
      display : inline-block;
      position : relative;
      right : 10px;
      top : 0;
    }

    a {
      color : $White;
      &:hover {
        color : $Orange;
      } 
      
    }
    
    // uncomment to reveal without jQuery
    /*
    ul li:hover > ul {
      display : block;
    }
    */
  }

