/*	^1 IMPORTS
*************************************************************************/

	@import url("css/reset.css");
	@import url(http://fonts.googleapis.com/css?family=Open+Sans:400,400italic,600,700,600italic,700italic);
	

/* ^2 SASS VARIABLES
*************************************************************************/
	
	// Colours
	$Black : #000;
	$DarkBlack : lighten($Black, 13%);
  $MediumBlack : lighten($Black, 20%);
  $MediumBlackLight : lighten($Black, 28%);
  $LightBlack : lighten($Black, 80%);
  $UltraLightBlack : lighten($Black, 95%);
  $White : #FFF;
  $Orange : #FF6600;
  $Purple : #6C276A;
  $Biege : #DAD6CA;
  
  // Globals
  $GlobalWidth : 1020px;
  $StickyHeight : 200px;
  $GlobalPadding : 20px;
  $GlobalLinkSize : 0.7em;
  
  $DarkDottedBorder : 1px dotted $DarkBlack;
  
  // Fonts
  @mixin serifFont {
    font-family : "Century", Georgia, "Times New Roman", Times, serif;
  }

  @mixin sansSerifFont {
    font-family : "Open Sans", Helvetica, Arial, sans-serif;
  }

  @mixin formFields {
    height : 15px;
    padding : 5px ;
    border : 0;
    @include sansSerifFont;
    font-size : $GlobalLinkSize;
  }
  
  @mixin arrowLinks {
    height : 25px;
    width : 25px;
    float : left;
    border : 0;
    background : $Orange url(img/sprite.png) no-repeat -83px -74px;
      &:hover {
        background-color : $LightBlack;
      }  
  }
  
  @mixin resetSpacing {
    padding : 0;
    margin : 0;
  }
  
  @mixin bringToFront {
    position : relative;
    z-index : 10;
  }

/* ^3	GLOBAL CLASSES
*************************************************************************/

	::-moz-selection {
		background : #b3d4fc;
		text-shadow : none;
	}
	
	::selection {
		background : #b3d4fc;
		text-shadow : none;
	}
	
	.clearfix:before,
	.clearfix:after {
		content : " ";
		display : table;
	}
	
	.clearfix:after {
		clear : both;
	}
	
	.oldbrowser {
		padding : 0.4em 0;
    margin : 0;
		text-align : center;
    background : $Orange;
    color : $White;
	}
	
	.oldbrowser a {
	  color : $Black;
	  &:hover {
	    color : $LightBlack;
	  }
	}
	
	hr {
		display : block;
		height : 1px;
		border : 0;
		border-top : 1px solid $MediumBlack;
		margin : 1em 0;
		padding : 0;
	}
	
	a {
	  text-decoration : none;
	}

  li a {
    transition : all 0.2s ease;
  }
	
	.hide {
	  display : none;
	}
	
  button[type=submit] {
    @include arrowLinks;
  }
      
  .single #MainContent p,
  .single #ThumbLinks a h4:last-child {
    color : $White;
  }
  
  .single #ThumbLinks {
    border-top-color : $LightBlack;
  }

  .news #wrapper,
  .single #wrapper,
  .gallery #wrapper {
    background : $MediumBlackLight;
  }

  .contributors-blog #wrapper {
    background : $Purple;
  }

 .highlight {
   color : $Orange;
 }
 
 .post-edit-link {

   padding : 5px $GlobalPadding;
   font-size : $GlobalLinkSize;
   font-weight : bold;
   color : $White;
   background : $Orange;
   
   &:hover {
     background : $LightBlack;
   }
 }

